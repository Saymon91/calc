<!DOCTYPE html>
<html lang="ru">
<head>
    <meta content="text/html" charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/calccalc/css/main.css') }}">
    {%  block styles %}{% endblock %}

    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/jquery-3.2.1.min.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/jquery.event.drag-2.3.0.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/jquery.event.drop-2.3.0.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/slick.core.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/slick.formatters.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/slick.dataview.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/slick.grid.js') }}"></script>
    <script type="application/javascript" src="{{ asset('bundles/calccalc/js/api.js') }}"></script>
    {%  block scripts %}{% endblock %}
    <title>{%  block title %}{% endblock %}</title>
</head>
<body>
<header>
    <section class="content">
        <h4>{%  block header %}{% endblock %}</h4>
    </section>
</header>
<nav>{%  block navigation %}{% endblock %}</nav>
<article class="content">
    {%  block content %}{% endblock %}
</article>
<footer>
    <div class="content">{%  block footer %}{% endblock %}</div>
</footer>
</body>
</html>